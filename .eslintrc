{
  "parser": "babel-eslint",
  "extends": ["@noths"],
  "rules": {
    "import/prefer-default-export": 0,
    "react/react-in-jsx-scope": 0,
    "react/jsx-filename-extension": 0,
    "react/prop-types": 0,
    "react/jsx-one-expression-per-line": 0,
    "react/destructuring-assignment": 0
  },
  "env": {
    "browser": true,
    "node": true,
    "jest": true
  }
}
